			"default": "./dist/components/schemaorg/breadcrumbs/JsonLdBreadcrumbs.svelte"
		},
		"./schemaorg/sitenavigationelements": {
			"types": "./dist/components/schemaorg/sitenavigationelements/index.d.ts",
