{
	"name": "@sveltinio/media-content",
	"version": "0.3.8",
	"private": false,
	"packageManager": "pnpm@7.29.0",
	"type": "module",
	"description": "A collection of type-safe Svelte components to easily embed media contents in your Svelte and SvelteKit projects.",
	"author": "dev <dev@sveltin.io>",
	"license": "MIT",
	"homepage": "https://components.sveltin.io",
	"keywords": [
		"svelte",
		"components",
		"svelte components",
		"sveltekit",
		"embed",
		"media",
		"media content",
		"content",
		"youtube",
		"youtube list",
		"youtube playlist",
		"facebook post",
		"facebook videos",
		"twitter",
		"tweet",
		"codepen",
		"stackblitz",
		"codesandbox",
		"vimeo",
		"slideshare"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sveltinio/components-library",
		"directory": "packages/media-content"
	},
	"bugs": {
		"url": "https://github.com/sveltinio/components-library/issues"
	},
	"dependencies": {
		"svelte": "^3.56.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.11.0",
		"@sveltejs/package": "^1.0.2",
		"@sveltejs/vite-plugin-svelte": "^2.0.3",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/jest": "^27.5.2",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"c8": "^7.13.0",
		"eslint": "^8.36.0",
		"eslint-config-custom": "workspace:0.0.0",
		"jsdom": "^21.1.1",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.8.3",
		"postcss-utils": "workspace:0.0.0",
		"publint": "^0.1.10",
		"rimraf": "^4.4.0",
		"svelte-check": "^3.1.4",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vitest": "^0.29.2"
	},
	"scripts": {
		"build": "pnpm clean && pnpm sync && svelte-package && pnpm postbuild",
		"clean": "rimraf dist",
		"dev": "svelte-kit sync && vite dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir=. --check . && eslint .",
		"format": "prettier --plugin-search-dir=. --write .",
		"postbuild": "pnpm exec publint ./dist",
		"sync": "svelte-kit sync",
		"test": "pnpm sync && vitest run",
		"test:coverage": "pnpm sync && vitest run --coverage",
		"test:watch": "pnpm sync && vitest watch"
	},
	"types": "index.d.ts",
	"svelte": "index.js",
	"exports": {
		".": {
			"types": "./types.d.ts",
			"import": "./index.js"
		},
		"./dist": "./index.js"
	}
}
