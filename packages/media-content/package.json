{
	"name": "@sveltinio/media-content",
	"version": "0.3.2",
	"private": false,
	"description": "A collection of typed Svelte components to easily embed media contents in your pages.",
	"author": "dev <dev@sveltin.io>",
	"license": "MIT",
	"keywords": [
		"svelte",
		"components",
		"embed",
		"media",
		"content",
		"oembed",
		"youtube",
		"youtube list",
		"youtube playlist",
		"facebook post",
		"facebook videos",
		"twitter",
		"tweet",
		"codepen",
		"stackblitz",
		"codesandbox",
		"vimeo",
		"slideshare"
	],
	"svelte": "index.js",
	"sideEffects": false,
	"publishConfig": {
		"directory": "dist"
	},
	"scripts": {
		"dev": "vite dev",
		"test": "vitest run",
		"test:watch": "vitest watch --reporter=default",
		"coverage": "vitest run --coverage",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"package": "npm run clean && svelte-kit package && npm run postpack",
		"clean": "rimraf dist",
		"postpack": "rimraf dist/__tests__",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.60",
		"@sveltejs/kit": "1.0.0-next.375",
		"@sveltejs/vite-plugin-svelte": "^1.0.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.3",
		"@types/jest": "^27.5.2",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"c8": "^7.11.3",
		"eslint": "^8.19.0",
		"jsdom": "latest",
		"micromatch": "^4.0.5",
		"rimraf": "^3.0.2",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.11",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"vitest": "latest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sveltinio/components-library"
	},
	"bugs": {
		"url": "https://github.com/sveltinio/components-library/issues"
	},
	"type": "module"
}
