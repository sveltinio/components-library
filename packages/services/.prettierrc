{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"bracketSpacing": true,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": ["*.svelte"],
			"options": {
				"bracketSameLine": false,
				"parser": "svelte"
			}
		}
	]
}
