{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"types": ["vitest/globals"]
	},
	"include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.svelte"],
	"exclude": ["tests"]
}
