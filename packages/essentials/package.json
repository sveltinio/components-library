		"check": "pnpm sync && svelte-check",
		"check:watch": "pnpm sync && svelte-check --watch",
		"clean": "rimraf dist",
		"clean:dist": "node ../../cleanDist.mjs packages/essentials/dist/components",
		"dev": "pnpm sync && pnpm dev:css && vite dev",
