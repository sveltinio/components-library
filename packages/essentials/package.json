		"minify": "cross-env NODE_ENV=production node ./scripts/css-minifier.js",
		"postbuild": "pnpm clean:dist && pnpm exec publint . && pnpm minify",
		"sync": "svelte-kit sync",
		"test": "pnpm build && vitest run",
