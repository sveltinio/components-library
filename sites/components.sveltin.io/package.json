		"@sveltinio/widgets": "workspace:0.1.7"
	},
	"devDependencies": {
		"@indaco/svelte-iconoir": "^2.5.1",
		"@sveltejs/adapter-static": "1.0.0-next.42",
		"@sveltejs/kit": "1.0.0-next.477",
		"@tailwindcss/aspect-ratio": "^0.4.2",
